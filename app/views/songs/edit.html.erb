<%= form_for @song do | f | %>
    <label> My song name </label><br>
    <%= f.text_field :name %><br>

    <label> My song artist </label><br>
    <%= f.text_field :artist_id %><br> 

    <label> My song genre </label><br>
    <%= f.text_field :genre_id %><br> 

    <%= f.submit "Update Song" %>
<% end %>